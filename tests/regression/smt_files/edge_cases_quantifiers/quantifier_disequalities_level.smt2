(declare-sort P 0)
(declare-const t P)
(declare-fun v (P) P)
(declare-fun l (P) P)
(declare-fun vs (P) P)
(assert (forall ((s P) (a P)) (! (= (v s) (v (vs a))) :pattern ((v s) (vs a)))))
(assert (forall ((x! P)) (! (= (l x!) (vs x!)) :pattern ((l x!)))))
(declare-const x P)
(assert (or (not (= t (v (l x)))) (not (= (v (l (l x))) t))))
(check-sat)