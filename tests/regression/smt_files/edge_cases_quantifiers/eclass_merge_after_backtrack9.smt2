(declare-const x Bool)
(declare-sort P 0)
(declare-sort L 0)
(declare-fun n (L) P)
;(declare-fun n (L) P)
(declare-fun % (P) L)
(declare-const stoy P)
(declare-fun b! (P) Bool)
(declare-fun L (P) Bool)
(declare-const tis L)
(assert (forall ((s P)) (! (= (b! s) (L (n (% s)))) :pattern ((b! s)))))
; instantiation (assert (= (b! stoy) (L (n (% stoy)))))
(declare-fun c (P) Bool)
(declare-const m L)
; these two not necessary for unsat, but necessary for making Sundance unknown
(assert (or x (= m tis)))
(assert (not (c (n m))))
(assert (= (% stoy) tis))
(assert (not (L (n tis))))
(assert (b! stoy))
(assert x)
(check-sat)