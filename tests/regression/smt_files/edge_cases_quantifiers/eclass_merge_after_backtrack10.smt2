(declare-const x Bool)
(declare-sort P 0)
(declare-sort L 0)
(declare-fun n (L) P)
(declare-fun % (P) L)
(declare-const stoy P)
(declare-fun b! (P) Bool)
(declare-fun L (P) Bool)
(declare-const i L)
(assert (forall ((s P)) (! (= (b! stoy) (L (n (% s)))) :pattern ((b! s)))))
(declare-fun c (P) Bool)
(declare-const m L)
(assert (or x (= m i)))
(assert (not (c (n m))))
(assert (= i (% stoy)))
(assert (not (L (n i))))
(assert (b! stoy))
(check-sat)
