(declare-datatypes ((c 0) (k 0)) (((c) (co (r P))) ((k (b! c)))))
(declare-fun y (k) P)
(declare-fun o (P) k)
(assert (forall ((x k)) (! (= x (o (y x))) :pattern ((y x)))))
(declare-fun ib (k P k) Bool)
(declare-fun b () P)
