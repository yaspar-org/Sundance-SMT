(declare-sort P 0)
(declare-fun I (Int) P)
(declare-fun h (P) Bool)
(declare-fun i (P) P)
(declare-fun b (P) P)
(declare-fun b1 (P) Int)
(declare-fun P (P) P)
(declare-fun v (P) P)
(declare-fun l (P P) P)
(assert (forall ((r P)) (! (h (P (i r))) :pattern ((i r)))))
(assert (forall ((f P)) (! (h (v f)) :pattern ((v f)))))
(assert (forall ((t P) (e P)) (! (= (v t) (l t e)) :pattern ((l t e)))))
(assert (not (h (b (v (I 0))))))
(assert (= (I 0) (i (b (I 0)))))
(assert (= (b (I 0)) (l (I 0) (I (b1 (I 0))))))
(assert (= (I 0) (P (I 0))))
(assert (= (v (I 0)) (i (l (I 0) (I (b1 (v (I 0))))))))
(check-sat)
